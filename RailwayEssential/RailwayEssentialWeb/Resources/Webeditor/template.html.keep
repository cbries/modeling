<body>

<div id="editMenu" style="display: none; margin-top: 10px; margin-bottom: 10px; float: left;">

<div style="width: 100%; display: table;">
    <div style="display: table-row">
        <div style="display: table-cell;"> 
			<div style="vertical-align: middle; width: 120px;">
			  <select name="webmenuCategories" id="webmenuCategories" style="width: 100px; vertical-align: middle;">
				{{TRACKSYMBOLCATEGORIES}}
			  </select>
			</div>
		</div>
        
		<div style="display: table-cell;">
		  {{TRACKSYMBOLS_Track}}
		  {{TRACKSYMBOLS_Switch}}
		  {{TRACKSYMBOLS_Signal}}
		  {{TRACKSYMBOLS_Block}}
		  {{TRACKSYMBOLS_Sensor}}
		  {{TRACKSYMBOLS_Accessory}}
		</div>

		<div style="display: table-cell;">
		  <fieldset style="margin-left: 15px; vertical-align: middle;">
			<label for="mode-1">Add/Move</label>
			<input type="radio" name="mode" id="mode-1" checked>

			<label for="mode-2">Remove</label>
			<input type="radio" name="mode" id="mode-2">

			<label for="mode-3">Rotate</label>
			<input type="radio" name="mode" id="mode-3">

			<label for="mode-4">Object Assignment</label>
			<input type="radio" name="mode" id="mode-4">
		  </fieldset>
		</div>
    </div>
</div>

</div>

