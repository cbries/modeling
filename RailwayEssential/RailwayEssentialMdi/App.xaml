<Application x:Class="RailwayEssentialMdi.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:views="clr-namespace:RailwayEssentialMdi.Views"
             xmlns:viewModels="clr-namespace:RailwayEssentialMdi.ViewModels"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <DataTemplate DataType="{x:Type viewModels:LogWindow}">
            <views:LogView />
        </DataTemplate>
        <DataTemplate DataType="{x:Type viewModels:TrackWindow}">
            <views:TrackView/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type viewModels:LocomotivesWindow}">
            <views:LocomotivesView/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type viewModels:PropertiesWindow}">
            <views:PropertiesView/>
        </DataTemplate>
    </Application.Resources>
</Application>
